{
  "name": "interauth",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@8.15.9",
  "engines": {
    "node": ">=18",
    "pnpm": ">=8"
  },
  "scripts": {
    "reinstall:local": "powershell -NoProfile -ExecutionPolicy Bypass -File ./scripts/reinstall.ps1",
    "dev": "vite dev",
    "build": "vite build",
    "start": "npm run preview",
    "preview": "vite preview --host 0.0.0.0 --port 3000",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^4.0.0",
    "@sveltejs/kit": "^2.0.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0",
    "svelte-check": "^4.0.0",
    "tailwindcss": "^3.4.0",
    "tslib": "^2.4.1",
    "typescript": "^5.0.0",
    "vite": "^5.0.0"
  },
  "dependencies": {
    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/auth-ui-svelte": "^0.2.9",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.39.0"
  },
  "type": "module"
}
