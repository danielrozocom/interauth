# Example .env (DO NOT commit secrets)

# Supabase (replace with your project values)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here

# Vite client envs (if your frontend uses them)
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# Application identifier used by the server for logging/identification
APP_NAME=InterAuth
VITE_APP_NAME=InterAuth

# URLs de callback para cada aplicaci√≥n
VITE_POS_CALLBACK_URL=https://pos.interfundeoms.edu.co/auth/callback
VITE_APP_CALLBACK_URL=https://app.interfundeoms.edu.co/auth/callback

# URLs de callback para desarrollo local (opcional)
VITE_POS_CALLBACK_URL_DEV=http://localhost:5174/auth/callback

# URLs base de las aplicaciones para fallback
VITE_POS_URL=https://pos.interfundeoms.edu.co
VITE_APP_URL=https://app.interfundeoms.edu.co

# URL de origen de InterAuth
VITE_AUTH_ORIGIN=https://auth.interfundeoms.edu.co
VITE_APP_CALLBACK_URL_DEV=http://localhost:5175/auth/callback
